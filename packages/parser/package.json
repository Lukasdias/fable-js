{
  "name": "@fable-js/parser",
  "version": "1.0.0",
  "description": "DSL parser for FableJS interactive storytelling",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/types.d.ts"
    }
  },
  "scripts": {
    "grammar": "ohm generateBundles --withTypes --esm 'src/grammar/*.ohm'",
    "build": "pnpm grammar && rollup -c",
    "dev": "rollup -c -w",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "dsl",
    "parser",
    "fable",
    "storytelling",
    "interactive",
    "education"
  ],
  "author": "Lukasdias",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lukasdias/fable-js",
    "directory": "packages/parser"
  },
  "homepage": "https://github.com/lukasdias/fable-js#readme",
  "dependencies": {
    "ohm-js": "^17.3.0"
  },
  "devDependencies": {
    "@ohm-js/cli": "^2.0.1",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "rollup": "^4.0.0",
    "typescript": "^5.6.0",
    "vitest": "^4.0.17"
  }
}
