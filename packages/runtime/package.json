{
  "name": "@fable-js/runtime",
  "version": "1.0.0",
  "description": "React runtime engine for FableJS interactive storytelling",
  "private": true,
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./src/index.d.ts"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "react",
    "interactive",
    "storytelling",
    "runtime",
    "engine",
    "fable"
  ],
  "author": "Lukasdias",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lukasdias/fable-js",
    "directory": "packages/runtime"
  },
  "homepage": "https://github.com/lukasdias/fable-js#readme",
  "dependencies": {
    "@fable-js/parser": "workspace:*",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-konva": "^19.0.0",
    "konva": "^9.3.0",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-babel": "^6.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "typescript": "^5.0.0",
    "rollup": "^4.0.0",
    "vitest": "^4.0.17",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "tslib": "^2.6.0"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
